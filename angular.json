{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "projects": {
    "rmzwallet": {
      "projectType": "application",
      "root": "",
      "sourceRoot": "src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/rmzwallet",
            "index": "src/index.html",
            "main": "src/main.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "tsconfig.json",
            "assets": [
              "src/favicon.ico",
              "src/assets",
              "src/manifest.webmanifest",
              {
                "glob": "**/*",
                "input": "public",
                "output": "/"
              }
            ],
            "serviceWorker": true,
            "ngswConfigPath": "ngsw-config.json",
            "styles": [],
            "scripts": [],
            "allowedCommonJsDependencies": [
              "qrcode",
              "chronik-client",
              "minimal-xec-wallet",
              "ecashaddrjs",
              "ecash-wallet",
              "bip39",
              "@noble/hashes/pbkdf2",
              "@noble/hashes/sha256",
              "@noble/hashes/sha512",
              "@noble/hashes/utils"
            ]
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ],
              "optimization": {
                "scripts": true,
                "styles": {
                  "minify": true,
                  "inlineCritical": true
                }
              },
              "outputHashing": "all",
              "sourceMap": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "serviceWorker": true,
              "ngswConfigPath": "ngsw-config.json",
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "3mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ],
              "allowedCommonJsDependencies": [
                "qrcode",
                "chronik-client",
                "minimal-xec-wallet",
                "ecashaddrjs",
                "ecash-wallet",
                "bip39",
                "@noble/hashes/pbkdf2",
                "@noble/hashes/sha256",
                "@noble/hashes/sha512",
                "@noble/hashes/utils"
              ]
            },
            "development": {
              "optimization": false,
              "sourceMap": {
                "scripts": false,
                "vendor": false
              },
              "vendorChunk": true,
              "extractLicenses": false,
              "buildOptimizer": false,
              "serviceWorker": false,
              "ngswConfigPath": "ngsw-config.json",
              "allowedCommonJsDependencies": [
                "qrcode",
                "chronik-client",
                "minimal-xec-wallet",
                "ecashaddrjs",
                "ecash-wallet",
                "bip39",
                "@noble/hashes/pbkdf2",
                "@noble/hashes/sha256",
                "@noble/hashes/sha512",
                "@noble/hashes/utils"
              ]
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "buildTarget": "rmzwallet:build"
          },
          "configurations": {
            "production": {
              "buildTarget": "rmzwallet:build:production"
            },
            "development": {
              "buildTarget": "rmzwallet:build:development"
            }
          },
          "defaultConfiguration": "development"
        }
      }
    }
  },
  "cli": {
    "analytics": false
  }
}
